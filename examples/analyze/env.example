# Tinybird API Token (required)
# Get from: Tinybird Dashboard > Tokens
TINYBIRD_TOKEN=your_token_here

# Use Tinybird Local? (true/false)
# Set to true for local development with Tinybird Local container
TINYBIRD_LOCAL=false

# Tinybird Local port (default: 7181)
TINYBIRD_PORT=7181

# Optional: Specify region if not auto-detected from token
# TINYBIRD_REGION=gcp-europe-west2

