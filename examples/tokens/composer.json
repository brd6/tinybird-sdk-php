{
    "name": "brd6/tinybird-sdk-php-example-tokens",
    "description": "Tinybird SDK PHP - Tokens API Example",
    "type": "project",
    "require": {
        "php": "^8.1",
        "brd6/tinybird-sdk-php": "*",
        "vlucas/phpdotenv": "^5.6",
        "symfony/http-client": "^7.4",
        "nyholm/psr7": "^1.8"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../"
        }
    ],
    "autoload": {
        "psr-4": {}
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
