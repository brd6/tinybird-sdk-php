# Tinybird Token (must have ADMIN or TOKENS scope)
TINYBIRD_TOKEN=p.eyJ...

# Set to true for Tinybird Local
TINYBIRD_LOCAL=false

# Region (only used when TINYBIRD_LOCAL=false)
# Options: gcp-europe-west2, gcp-europe-west3, gcp-us-east4, etc.
TINYBIRD_REGION=gcp-europe-west3

# Local port (only used when TINYBIRD_LOCAL=true)
TINYBIRD_PORT=7181

